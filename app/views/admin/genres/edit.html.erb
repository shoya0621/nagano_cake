<div class="container">
 <h4 class="my-4">ジャンル編集</h4>
 <div class="row py-5">
  <h5 class="col-2">ジャンル名</h5>
  <%= form_with model: @genre,url: admin_genre_path(@genre.id), method: :patch do |f| %>
   <%= f.text_field :name,class: "align-middle"%>
   <%= f.submit "変更を保存",class: "btn btn-sm btn-success ml-3" %>
 <% end %>
 </div>
</div>